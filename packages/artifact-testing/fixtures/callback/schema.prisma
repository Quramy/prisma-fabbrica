generator client {
  provider = "prisma-client-js"
  output   = "./__generated__/client"
}

generator fabbrica {
  provider    = "prisma-fabbrica"
  output      = "./__generated__/fabbrica"
  noTranspile = true
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id
  name  String
}
